
(*
	Fibbonacci.
	Requires the stdlib in lib/stdlib.kha
*)

sig int -> int in
let fib = λx : int => 6

sig int -> int in
let fib =
    λn : int =>
    if n = 0 then
       n
    else if n = 1 then
       n
    else
       (fib (n - 1)) + (fib (n - 2))

sig () -> () in
let main = λ() : () => show (fib 35)
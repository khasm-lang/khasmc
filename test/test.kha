fun main (m : Int): Int =
        let Left x = interesting 10 in
        let y = Record {
                name = "ho";
        } in
        x

type Either 'a 'b =
        | Left of 'a
        | Right of 'b
end

type Record 'a = {
        name : 'a;
}


fun interesting {type 'a} (x : Int) : Either Int 'a = Left x

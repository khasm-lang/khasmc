fun main (m : Int): Int =
        let Right x = swap (interesting 10) in
        let y = Record {
                name = "ho";
        } in
        x

type Either 'a 'b =
        | Left of 'a
        | Right of 'b
end

type Record 'a = {
        name : 'a;
}


fun interesting {type 'a} (x : Int) : Either Int 'a = Left x

fun swap {type 'a 'b 'c} (e : Either 'a (Either 'b 'c)) : Either (Either 'a 'b) 'c =
        match e with
        | Left x => Left (Left x)
        | Right (Left x) => Left (Right x)
        | Right (Right x) => Right x
        end 